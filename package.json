{
	"name": "portfolio_website",
	"version": "0.0.9",
	"scripts": {
		"start": "node --max-http-header-size=16384 server.js",
		"heroku-postbuild": "cd client && yarn cache clean && yarn && yarn run build",
		"client": "cd client && yarn start",
		"server": "nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
	},
	"dependencies": {
		"body-parser": "^1.18.3",
		"dotenv": "^10.0.0",
		"express": "^4.16.4",
		"express-rate-limit": "^5.5.1",
		"mongoose": "6.3.0",
		"queue-microtask": "^1.2.3",
		"call-bind": "1.0.2",
		"typescript": "~4.5.5"
	},
	"devDependencies": {
		"concurrently": "^7.0.0",
        "@types/express-rate-limit": "6.0.0"
	},
    "config": {
        "mongodbMemoryServer": {
            "version": "4.4.1"
        }
    },
	"author": "Gihun Ko",
	"license": "MIT",
	"engines": {
		"node": "16.0.0"
	}
}
