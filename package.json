{
	"name": "portfolio_website",
	"version": "0.0.9",
	"scripts": {
		"start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
		"heroku-postbuild": "cd client && yarn cache clean && yarn && yarn run build",
		"client": "cd client && yarn start",
		"client-dev": "cd client && yarn dev",
		"server": "nodemon --max-http-header-size=16384 server.js",
		"server-dev": "nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"yarn server-dev\" \"yarn client-dev\""
	},
	"dependencies": {
		"body-parser": "^1.20.1",
		"call-bind": "1.0.2",
		"dotenv": "^16.0.2",
		"express": "^4.18.1",
		"express-rate-limit": "^6.6.0",
		"mongoose": "6.6.1",
		"queue-microtask": "^1.2.3"
	},
	"devDependencies": {
		"@types/express-rate-limit": "6.0.0",
		"concurrently": "^7.4.0"
	},
	"config": {
		"mongodbMemoryServer": {
			"version": "4.4.1"
		}
	},
	"author": "Gihun Ko",
	"license": "MIT"
}
